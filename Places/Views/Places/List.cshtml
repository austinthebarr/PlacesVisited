<!DOCTYPE html>
<html>
  <head>
    <title>Your Places</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <style>
    body
    {
      background-color: lightgreen;
    }
    </style>
  </head>
  <body>
    <div class="container">
    <h1>Your Places</h1>
    @foreach(var place in Model)
    {
      <p>@place.GetLocation()</p>
      <p>@place.GetDates()</p><br>
      <img style="height:150px" src="@place.GetPicture()">

    }
    <br>
    <a href="/places/add">Add New Place</a><br>
    <a href="/">Home</a>
    </div>
  </body>
</html>
